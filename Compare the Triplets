#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int a[3],b[3],alice=0,bob=0,i;
    scanf("%d %d %d",&a[0],&a[1],&a[2]);
    scanf("%d %d %d",&b[0],&b[1],&b[2]);
    for(i=0;i<3;i++)
        {
        if(a[i]>b[i])
            alice++;
        else if(b[i]>a[i])
            bob++;
    }
    printf("%d %d",alice,bob);
    return 0;
}
